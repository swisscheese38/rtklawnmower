Visualization Manager:
  Global Options:
    Fixed Frame: base_link
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 2
  Displays:
    - Class: rviz/Grid
      Enabled: true
    - Class: rviz/RobotModel
      Enabled: true
      Alpha: 0.5
      Value: true
      Links:
        left_wheel:
          Show Axes: true
        right_wheel:
          Show Axes: true
<?xml version="1.0"?>
<launch>

  <!-- Robot description -->
  <include file="$(find rtklm_description)/launch/description.launch" />

  <!-- Start sensors/actors -->
  <include file="$(find rtklm_bringup)/launch/gps.launch"/>
  <include file="$(find rtklm_bringup)/launch/i2c.launch"/>
  <include file="$(find rtklm_bringup)/launch/arduino.launch"/>

  <!-- Start DiffDrive controller -->
  <include file="$(find rtklm_bringup)/launch/control.launch"/>

  <!-- Fuse GPS, Wheel odometry and IMU -->
  <include file="$(find rtklm_localization)/launch/localization.launch"/>

</launch>
